  "mcp_servers": {
    "registry": "registry.modelcontextprotocol.io",
    "description": "Model Context Protocol服务器",
    "installation_methods": [
      {
        "name": "HTTP Server",
        "command": "claude mcp add --transport http <name> <url>",
        "description": "连接到远程HTTP MCP服务器"
      },
      {
        "name": "SSE Server",
        "command": "claude mcp add --transport sse <name> <url>",
        "description": "连接到Server-Sent Events服务器（已弃用）"
      },
      {
        "name": "Stdio Server",
        "command": "claude mcp add --transport stdio <name> -- <command>",
        "description": "连接到本地stdio MCP服务器"
      }
    ],
